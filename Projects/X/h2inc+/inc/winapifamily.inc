ifndef _INC_WINAPIFAMILY
  _INC_WINAPIFAMILY equ <>
  if Defined(_MSC_VER) and  not Defined(MOFCOMP_PASS)
    if _MSC_VER ge 1200
    endif
  endif
  include winpackagefamily.inc
  WINAPI_FAMILY_PC_APP equ 2
  WINAPI_FAMILY_PHONE_APP equ 3
  WINAPI_FAMILY_SYSTEM equ 4
  WINAPI_FAMILY_SERVER equ 5
  WINAPI_FAMILY_GAMES equ 6
  WINAPI_FAMILY_DESKTOP_APP equ 100
  WINAPI_FAMILY_APP equ <WINAPI_FAMILY_PC_APP>
  ifndef WINAPI_FAMILY
    WINAPI_FAMILY equ <WINAPI_FAMILY_DESKTOP_APP>
  endif
  if WINAPI_FAMILY ne WINAPI_FAMILY_DESKTOP_APP and WINAPI_FAMILY ne WINAPI_FAMILY_PC_APP and WINAPI_FAMILY ne WINAPI_FAMILY_PHONE_APP and WINAPI_FAMILY ne WINAPI_FAMILY_SYSTEM and WINAPI_FAMILY ne WINAPI_FAMILY_GAMES and WINAPI_FAMILY ne WINAPI_FAMILY_SERVER
    .err <Unknown WINAPI_FAMILY value. Was it defined in terms of a WINAPI_PARTITION_ * value?>
  endif
  ifndef WINAPI_PARTITION_DESKTOP
    WINAPI_PARTITION_DESKTOP equ (WINAPI_FAMILY eq WINAPI_FAMILY_DESKTOP_APP)
  endif
  ifndef WINAPI_PARTITION_APP
    WINAPI_PARTITION_APP equ (WINAPI_FAMILY eq WINAPI_FAMILY_DESKTOP_APP or WINAPI_FAMILY eq WINAPI_FAMILY_PC_APP or WINAPI_FAMILY eq WINAPI_FAMILY_PHONE_APP)
  endif
  ifndef WINAPI_PARTITION_PC_APP
    WINAPI_PARTITION_PC_APP equ (WINAPI_FAMILY eq WINAPI_FAMILY_DESKTOP_APP or WINAPI_FAMILY eq WINAPI_FAMILY_PC_APP)
  endif
  ifndef WINAPI_PARTITION_PHONE_APP
    WINAPI_PARTITION_PHONE_APP equ (WINAPI_FAMILY eq WINAPI_FAMILY_PHONE_APP)
  endif
  ifndef WINAPI_PARTITION_GAMES
    WINAPI_PARTITION_GAMES equ (WINAPI_FAMILY eq WINAPI_FAMILY_GAMES or WINAPI_FAMILY eq WINAPI_FAMILY_DESKTOP_APP)
  endif
  ifndef WINAPI_PARTITION_SYSTEM
    WINAPI_PARTITION_SYSTEM equ (WINAPI_FAMILY eq WINAPI_FAMILY_SYSTEM or WINAPI_FAMILY eq WINAPI_FAMILY_SERVER)
  endif
  WINAPI_PARTITION_PHONE equ <WINAPI_PARTITION_PHONE_APP>
  WINAPI_FAMILY_PARTITION macro Partitions
    exitm <(Partitions)>
  endm
  _WINAPI_DEPRECATED_DECLARATION equ <__declspec(deprecated ("This API cannot be used in the context of the caller's application type."))>
  if WINAPI_FAMILY_PARTITION(WINAPI_PARTITION_APP) and  not WINAPI_FAMILY_PARTITION(WINAPI_PARTITION_DESKTOP)
    APP_DEPRECATED_HRESULT equ <HRESULT _WINAPI_DEPRECATED_DECLARATION>
  endif
  if Defined(_MSC_VER) and  not Defined(MOFCOMP_PASS)
    if _MSC_VER ge 1200
    endif
  endif
endif
